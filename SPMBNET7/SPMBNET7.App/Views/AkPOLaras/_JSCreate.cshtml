    function JsonNoPO(id) {
        if (id != null) {
            $.ajax({
                type: "POST",
                //url: "/AkPOLaras/JsonGetNoPO",
                url: '@Url.Action("JsonGetNoPO", "AkPOLaras")',
                data: { id: id },
